---
import Layout from "../layouts/Layout.astro";
import "../styles.css";
import { Image, Picture } from "@astrojs/image/components";

import special_project from "../../public/Images/special_project.svg";
import overview from "../../public/Images/overview.png";
import nattyam from "../../public/Images/nattyam.svg";
import client_vision_mission from "../../public/Images/client_vision_mission.svg";
import gd_number_1 from "../../public/Images/gd_number_1.svg";
import gd_number_2 from "../../public/Images/gd_number_2.svg";
import gd_number_3 from "../../public/Images/gd_number_3.svg";
import gd_number_4 from "../../public/Images/gd_number_4.svg";
import bug_fix from "../../public/Images/bug_fix.svg";
import module from "../../public/Images/module.svg";
import end_to_end from "../../public/Images/end_to_end.svg";
import inefficiences_stock_tracking from "../../public/Images/inefficiences_stock_tracking.svg";
import sales_and_accounting from "../../public/Images/sales_and_accounting.svg";
import manufacturing_packaging from "../../public/Images/manufacturing_packaging.svg";
import gst_complexity from "../../public/Images/gst_complexity.svg";
import crm_pos from "../../public/Images/crm_pos.svg";
import gd_CRM from "../../public/Images/gd_CRM.svg";
import gd_Sales from "../../public/Images/gd_Sales.svg";
import gd_Purchase from "../../public/Images/gd_Purchase.svg";
import gd_Pos from "../../public/Images/gd_Pos.svg";
import gd_Manufacturing from "../../public/Images/gd_Manufacturing.svg";
import gd_Inventory from "../../public/Images/gd_Inventory.svg";
---

<Layout
    title="Tech Magic Behind GramDharaa Gaushala! | Sanskar Technolab"
    ,
    keyword="ERPNext partner, ERPNext development, ERP software development, ERPNext implementation services, ERPNext implementation partner in India, ERPNext development in India, ERP consulting services, ERP deployment services, ERP software development company, ERPNext Integration services,	ERPNext implementation partners, ERPNext implementation, erp implementation partner, erp implementation companies, ERPNext software development company, Hire ERPNext developers, ERPNext development services , Erpnext solution company, Frappe ERPNext , Erpnext software , ERPNext open-source, ServiceNow company, ServiceNow implementation partner, Servicenow implementation services, Servicenow implementation solutions"
    ,
    description="Find out how we used ERPNext to help our client in the dairy industry sync their operations effortlessly between their gaushalas and stores, making their process smoother and more efficient."
    ,
    canonical_href="https://sanskartechnolab.com/gramdharaa_gaushala"
>
    <!-- GramDharaa start  -->

    <section
        class="hidden lg:flex section-bg h-[90vh] xl:h-[75vh] flex items-center banner-bg"
    >
        <div
            class="flex flex-wrap items-center mx-5 md:max-w-7xl md:mx-auto md:px-24 2xl:px-0"
        >
            <div class="w-full md:w-6/12">
                <h1
                    class="text-[1.75rem] md:text-[3rem] leading-9 md:leading-[3.5rem] text-[#31393C] font-semibold"
                >
                    Our Special Project: Tech Magic Behind GramDharaa Gaushala!
                </h1>
                <p
                    class="text[#31393C] text-sm md:text-base leading-5 md:leading-6 mt-5"
                >
                    Find out how we used ERPNext to help our client in the dairy
                    industry sync their operations effortlessly between their
                    gaushalas and stores, making their process smoother and more
                    efficient.
                </p>
                <br />
                <p class="text-[18px] font-bold text-[#31393C] mt-2">
                    This is a very special project for us, keep scrolling to
                    know why.
                </p>
                <!-- <div
              class="mt-7 flex justify-between bg-[#3E96F4] rounded-lg px-3 md:px-3 lg:px-5 py-2 md:py-2 lg:py-3 md:w-72 sm:w-auto"
            >
              <a
                href="/contact-us"
                class="text-white text-base font-semibold font-inter"
                >Schedule a Free Demo Now</a
              >
              <Image
                loading="eager"
                src={right}
                width={6}
                height={12}
                alt="right arrow "
              />
            </div> -->
            </div>

            <Image
                loading="eager"
                src={special_project}
                alt="special_project"
                class="w-full md:w-6/12 lg:p-10 md:p-8"
            />
        </div>
    </section>

    <section class="section-bg flex items-center md:flex lg:hidden banner-bg">
        <div
            class="flex flex-wrap items-center mx-5 gap-5 md:max-w-7xl md:mx-auto md:px-24 2xl:px-0"
        >
            <div class="w-full lg:w-6/12 mt-5">
                <div
                    class="text-[1.5rem] md:text-[2.25rem] leading-9 md:leading-[3.5rem] text-[#31393C] font-semibold"
                >
                    Our Special Project: Tech Magic Behind GramDharaa Gaushala!
                </div>
                <p
                    class="text[#31393C] text-sm md:text-base leading-5 md:leading-6 mt-5"
                >
                    Find out how we used ERPNext to help our client in the dairy
                    industry sync their operations effortlessly between their
                    gaushalas and stores, making their process smoother and more
                    efficient.
                </p>
                <br />
                <p
                    class="md:text-[18px] text-[14px] font-bold text-[#31393C] mt-1"
                >
                    This is a very special project for us, keep scrolling to
                    know why.
                </p>
                <!-- <div
              class="mt-4 md:mt-7 flex justify-between bg-[#3E96F4] rounded-lg px-3 md:px-3 lg:px-5 py-2 md:py-2 lg:py-3 md:w-72 sm:w-auto"
            >
              <a
                href="/contact-us"
                class="text-white text-base font-semibold font-inter"
                >Schedule a Free Demo Now</a
              >
              <Image
                loading="eager"
                src={right}
                width={6}
                height={12}
                alt="right arrow"
              />
            </div> -->
            </div>

            <Image
                loading="eager"
                src={special_project}
                alt="overview"
                class="w-full mb-5"
            />
        </div>
    </section>
    <!-- GramDharaa end  -->

    <!-- general overview start -->

    <section
        class="section-bg-left_gramdhara py-[2rem] md:py-[2rem] lg:py-[4rem]"
    >
        <div class="max-w-[1300px] mx-auto md:px-5 px-3 lg:px-0">
            <h2
                class="text-[#3E96F4] text-2xl md:text-3xl lg:text-4xl font-semibold"
            >
                General Overview!
            </h2>
        </div>

        <div class="max-w-[1300px] mx-auto mt-5">
            <div class="lg:flex">
                <div class="lg:basis-6/12 md:px-5 px-3 lg:px-0">
                    <Image
                        loading="eager"
                        src={overview}
                        alt="overview"
                        class="lg:h-72 w-full"
                    />
                    <!-- <Image
                    loading="eager"
                    src={group_637}
                    alt="group_637"
                    class=""
		        /> -->
                </div>

                <div class="lg:basis-6/12 md:p-5 p-3">
                    <p class="text-[#31393C] text-xs md:text-sm lg:text-base">
                        GramDharaa Gaushala owns several gaushala stores,
                        selling cow-based products across India. They have
                        started NETTYAM, an initiative of Gramdharaa Gaushala
                        towards a healthy lifestyle by selling eco-friendly and
                        panchgavya products.
                    </p>

                    <p
                        class="text-[#31393C] text-xs md:text-sm lg:text-base mt-5"
                    >
                        Struggling to keep both gaushala and stores in sync,
                        along with inefficient inventory and logistics
                        management - they reached out to Sanskar Technolabs for
                        end-to-end management(right from manufacturing, and
                        inventory to sales and CRM).
                    </p>
                </div>
            </div>
        </div>
    </section>

    <!-- general overview end -->

    <!-- special project part start  -->
    <section class="py-[2rem] md:py-[2rem] lg:py-[4rem]">
        <div class="container mx-auto max-w-[1300px]">
            <h2
                class="overview lg:mx-2 md:mx-5 mx-3 text-2xl md:text-3xl lg:text-4xl"
            >
                Why this special project for Sanskar Technolabs?
            </h2>

            <div
                class="grid lg:mx-2 md:mx-5 mx-3 lg:grid-cols-4 md:grid-cols-2 grid-cols-1 mt-8"
            >
                <div>
                    <Image
                        loading="eager"
                        src={nattyam}
                        alt="nattyam"
                        class=""
                    />
                </div>
                <div>
                    <p
                        class="lg:text-xl md:text-lg text-sm lg:mt-0 md:mt-0 mt-10 font-semibold"
                    >
                        We didn't charge any money for this work!
                    </p>
                    <p class="text-xs md:text-sm lg:text-base mt-2">
                        It's part of our special CSR(Corporate Social
                        Responsibility) program to help our peers and make a
                        good impact in what we do best(technology and ERPNext).
                    </p>
                </div>
                <div class="lg:pl-10 lg:mt-0 md:mt-8 mt-7">
                    <p class="mb-1 font-bold lg:text-xl md:text-lg text-sm">
                        Client Location
                    </p>
                    <p class="mb-1 text-xs md:text-sm lg:text-base">
                        <span class="font-semibold">Office:</span> Sangli, Maharashtra
                    </p>
                    <p class="text-xs md:text-sm lg:text-base">
                        <span class="font-semibold">Goshala:</span> Ved Khillar Goshala,
                        Kolhapur, Maharashtra.
                    </p>
                    <p class="font-semibold mt-4 lg:text-xl md:text-lg text-sm">
                        Industry
                    </p>
                    <p class="text-xs md:text-sm lg:text-base">
                        Panchgavya Manufacturing
                    </p>
                </div>
                <div class="lg:mt-0 md:mt-8 mt-5">
                    <p class="lg:text-xl md:text-lg text-sm font-semibold">
                        Tech-Stack
                    </p>
                    <p class="text-xs md:text-sm lg:text-base">
                        Web Server: Frappe Cloud <br /> Database: MySQL (Maria DB)
                        <br /> ERPNext Version: 14.13.0
                    </p>

                    <p class="lg:text-xl md:text-lg text-sm font-semibold mt-5">
                        Project Methodology
                    </p>
                    <p class="text-xs md:text-sm lg:text-base">
                        Agile Scrum Methodology
                    </p>
                </div>
            </div>
        </div>
    </section>

    <!-- special project part end  -->

    <!-- Clients Vision & Mission! for lg screen start -->

    <section class="hidden lg:block py-[2rem] md:py-[2rem] lg:py-[4rem]">
        <div class="relative max-w-[1300px] mx-auto">
            <!-- <img
                src="client_vision_mission"
                alt="Your Image"
                class="w-full h-auto"
            /> -->
            <Image
                loading="eager"
                src={client_vision_mission}
                alt="client_vision_mission"
                class="w-full h-auto"
            />

            <div class="absolute top-0 text-black w-1/2 mt-20 ml-10">
                <h2
                    class="text-[#3E96F4] text-2xl md:text-3xl lg:text-4xl font-semibold"
                >
                    Clients Vision & Mission!
                </h2>

                <p class="text-[#31393C] text-xs md:text-sm lg:text-base mt-10">
                    Our client and founder of GramDharaa Gaushala are willing to
                    normalize the consumption and appliance of cow-based organic
                    products(in our day-to-day lives). However, to achieve his
                    not-so-normal vision, efficient and comprehensive management
                    is the key. To help them, boost efficiency and sales(of
                    course), we leveraged the one-of-kind features of ERPNext.
                </p>

                <p class="text-[#31393C] text-xs md:text-sm lg:text-base mt-10">
                    With ERPNext’s various features and modules - we have
                    streamlined (almost) every process for our client.
                </p>
            </div>
        </div>
    </section>

    <!-- Clients Vision & Mission! for lg screen end -->

    <!-- Clients Vision & Mission! for sm & md screen start -->

    <section class="lg:hidden py-[2rem]">
        <div class="mx-2 md:mx-5 bg-[#F6FBFF] rounded-xl p-3 md:p-0">
            <h2
                class="text-[#3E96F4] text-2xl md:text-3xl font-semibold md:p-5"
            >
                Clients Vision & Mission!
            </h2>

            <p
                class="text-[#31393C] text-xs md:text-sm lg:text-base md:px-5 md:mt-0 mt-2"
            >
                Our client and founder of GramDharaa Gaushala are willing to
                normalize the consumption and appliance of cow-based organic
                products(in our day-to-day lives). However, to achieve his
                not-so-normal vision, efficient and comprehensive management is
                the key. To help them, boost efficiency and sales(of course), we
                leveraged the one-of-kind features of ERPNext.
            </p>

            <p
                class="text-[#31393C] text-xs md:text-sm lg:text-base md:px-5 md:mt-4 mt-3"
            >
                With ERPNext’s various features and modules - we have
                streamlined (almost) every process for our client.
            </p>
        </div>
    </section>

    <!-- Clients Vision & Mission! for sm & md screen end -->

    <!-- features and number part start  -->
    <section class="py-[2rem] md:py-[2rem] lg:py-[4rem]">
        <div
            class="container max-w-[1300px] mx-auto grid grid-cols-1 lg:grid-cols-2 px-3 md:px-5 lg:px-0"
        >
            <div class="left">
                <h2
                    class="text-[#3E96F4] text-2xl md:text-3xl lg:text-4xl font-semibold"
                >
                    But, which features?
                </h2>
                <p
                    class="text-[#31393C] text-xs md:text-sm lg:text-base mt-3 md:mt-3 lg:mt-5"
                >
                    Our team has connected with our client to better understand
                    their needs, business processes, challenges and what
                    solutions are they targeting.
                </p>
                <p
                    class="text-[#31393C] text-xs md:text-sm lg:text-base mt-3 md:mt-3 lg:mt-5"
                >
                    After getting a thorough idea of their work and the
                    solutions needed, our team seamlessly integrated ERPNext
                    modules, managing manufacturing with precision, organizing
                    inventory management, optimizing purchasing processes,
                    enhancing customer relationships through CRM, and
                    facilitating swift transactions at the point of sale.
                </p>
                <p
                    class="text-[#31393C] text-xs md:text-sm lg:text-base mt-3 md:mt-3 lg:mt-5"
                >
                    The result is a cohesive system that blends the operations
                    of gaushalas and stores for a more efficient and
                    interconnected workflow.
                </p>
            </div>

            <div class="right mt-3 md:mt-5 lg:mt-0 lg:ml-10">
                <h2
                    class="text-[#3E96F4] text-2xl md:text-3xl lg:text-4xl font-semibold md:mt-3 mt-5"
                >
                    Here are the numbers!
                </h2>
                <div
                    class="grid md:grid-cols-2 bg-[#FFFFFF] gap-5 mt-5 container mx-auto md:mx-0"
                >
                    <div
                        class="container bg-[#ECF5FF] shadow-right shadow-bottom shadow-lg rounded-lg p-5"
                    >
                        <div class="flex justify-end">
                            <Image
                                loading="eager"
                                src={gd_number_1}
                                alt="gd_number_1"
                                class=""
                            />
                        </div>
                        <p class="text-[#31393C] text-[64px] font-bold">24%</p>
                        <p
                            class="text-[#31393C] text-xs md:text-sm lg:text-base"
                        >
                            GramDharaa Gaushala experienced a significant boost
                            of 24% in sales.
                        </p>
                    </div>

                    <div
                        class="container bg-[#ECF5FF] shadow-right shadow-bottom shadow-lg rounded-lg p-5 md:mt-0 mt-2"
                    >
                        <div class="flex justify-end">
                            <Image
                                loading="eager"
                                src={gd_number_2}
                                alt="gd_number_2"
                                class=""
                            />
                        </div>
                        <p class="text-[#31393C] text-[64px] font-bold">36%</p>
                        <p
                            class="text-[#31393C] text-xs md:text-sm lg:text-base"
                        >
                            Our streamlined processes didn't just save time but
                            supercharged employee efficiency by 36%.
                        </p>
                    </div>

                    <div
                        class="container bg-[#ECF5FF] shadow-right shadow-bottom shadow-lg rounded-lg p-5 md:mt-0 mt-2"
                    >
                        <div class="flex justify-end">
                            <Image
                                loading="eager"
                                src={gd_number_3}
                                alt="gd_number_3"
                                class=""
                            />
                        </div>
                        <p class="text-[#31393C] text-[64px] font-bold">98%</p>
                        <p
                            class="text-[#31393C] text-xs md:text-sm lg:text-base"
                        >
                            With ERPNext, GramDharaa Gaushala now enjoys an
                            impressive 98% accuracy with inventory management.
                        </p>
                    </div>

                    <div
                        class="container bg-[#ECF5FF] shadow-right shadow-bottom shadow-lg rounded-lg p-5 md:mt-0 mt-2"
                    >
                        <div class="flex justify-end">
                            <Image
                                loading="eager"
                                src={gd_number_4}
                                alt="gd_number_4"
                                class=""
                            />
                        </div>
                        <p class="text-[#31393C] text-[64px] font-bold">92%</p>
                        <p
                            class="text-[#31393C] text-xs md:text-sm lg:text-base"
                        >
                            The enhanced CRM features contributed to an
                            impressive 92% customer satisfaction rate.
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- feature and numbers part end  -->

    <!-- slider start -->

    <section
        class="mx-5 md:max-w-7xl md:mx-auto md:px-24 2xl:px-0 py-[2rem] md:py-[2rem] lg:py-[4rem]"
    >
        <div class="md:text-center sm:text-left">
            <h2
                class="text-2xl md:text-3xl lg:text-4xl text-[#3E96F4] font-semibold mb-2 md:mb-3 lg:mb-4 mx-2 md:text-center"
            >
                The ERPNext Modules we have implemented for GramDharaa Gaushala!
            </h2>
            <!-- </div> -->

            <div
                id="carouselExampleIndicators"
                class="relative"
                data-te-carousel-init
                data-te-carousel-slide
                data-te-ride="carousel"
            >
                <!--Carousel indicators-->
                <div
                    class="absolute -bottom-[3.5rem] md:-bottom-[4rem] lg:-bottom-[4rem] left-0 right-0 z-[2] mx-[15%] mb-4 flex list-none justify-center p-0 gap-4"
                    data-te-carousel-indicators
                >
                    <button
                        type="button"
                        data-te-target="#carouselExampleIndicators"
                        data-te-slide-to="0"
                        data-te-carousel-active
                        class="mx-[3px] box-content h-3 md:h-4 lg:h-4 w-3 md:w-4 lg:w-4 rounded-full flex-initial cursor-pointer border-solid border-transparent bg-[#D9D9D9] bg-clip-padding p-0 -indent-[999px] opacity-50 transition-opacity duration-[600ms] ease-[cubic-bezier(0.25,0.1,0.25,1.0)] motion-reduce:transition-none"
                        aria-current="true"
                        aria-label="Slide 1"></button>
                    <button
                        type="button"
                        data-te-target="#carouselExampleIndicators"
                        data-te-slide-to="1"
                        class="mx-[3px] box-content h-3 md:h-4 lg:h-4 w-3 md:w-4 lg:w-4 rounded-full flex-initial cursor-pointer border-solid border-transparent bg-[#D9D9D9] bg-clip-padding p-0 -indent-[999px] opacity-50 transition-opacity duration-[600ms] ease-[cubic-bezier(0.25,0.1,0.25,1.0)] motion-reduce:transition-none"
                        aria-label="Slide 2"></button>
                    <button
                        type="button"
                        data-te-target="#carouselExampleIndicators"
                        data-te-slide-to="2"
                        class="mx-[3px] box-content h-3 md:h-4 lg:h-4 w-3 md:w-4 lg:w-4 rounded-full flex-initial cursor-pointer border-solid border-transparent bg-[#D9D9D9] bg-clip-padding p-0 -indent-[999px] opacity-50 transition-opacity duration-[600ms] ease-[cubic-bezier(0.25,0.1,0.25,1.0)] motion-reduce:transition-none"
                        aria-label="Slide 3"></button>
                    <button
                        type="button"
                        data-te-target="#carouselExampleIndicators"
                        data-te-slide-to="3"
                        class="mx-[3px] box-content h-3 md:h-4 lg:h-4 w-3 md:w-4 lg:w-4 rounded-full flex-initial cursor-pointer border-solid border-transparent bg-[#D9D9D9] bg-clip-padding p-0 -indent-[999px] opacity-50 transition-opacity duration-[600ms] ease-[cubic-bezier(0.25,0.1,0.25,1.0)] motion-reduce:transition-none"
                        aria-label="Slide 4"></button>
                    <button
                        type="button"
                        data-te-target="#carouselExampleIndicators"
                        data-te-slide-to="4"
                        class="mx-[3px] box-content h-3 md:h-4 lg:h-4 w-3 md:w-4 lg:w-4 rounded-full flex-initial cursor-pointer border-solid border-transparent bg-[#D9D9D9] bg-clip-padding p-0 -indent-[999px] opacity-50 transition-opacity duration-[600ms] ease-[cubic-bezier(0.25,0.1,0.25,1.0)] motion-reduce:transition-none"
                        aria-label="Slide 5"></button>
                    <button
                        type="button"
                        data-te-target="#carouselExampleIndicators"
                        data-te-slide-to="5"
                        class="mx-[3px] box-content h-3 md:h-4 lg:h-4 w-3 md:w-4 lg:w-4 rounded-full flex-initial cursor-pointer border-solid border-transparent bg-[#D9D9D9] bg-clip-padding p-0 -indent-[999px] opacity-50 transition-opacity duration-[600ms] ease-[cubic-bezier(0.25,0.1,0.25,1.0)] motion-reduce:transition-none"
                        aria-label="Slide 6"></button>
                </div>

                <!--Carousel items-->
                <div
                    class="relative w-full overflow-hidden after:clear-both after:block after:content-['']"
                >
                    <!--First item-->
                    <div
                        class="relative float-left -mr-[100%] w-full transition-transform duration-[600ms] ease-in-out motion-reduce:transition-none"
                        data-te-carousel-item
                        data-te-carousel-active
                    >
                        <div
                            class="sm:min-h-[31rem] md:min-h-[45rem] lg:min-h-[27rem] block flex flex-wrap mt-5 gap-5"
                        >
                            <Image
                                loading="eager"
                                class="sm:w-full md:w-full lg:w-7/12"
                                src={gd_Purchase}
                                alt="slider image 1"
                            />

